{
  "type": "module",
  "packageManager": "bun@1.1.18",
  "engines": {
    "bun": ">=1.1.0",
    "node": ">=22.0.0"
  },
  "scripts": {
    "DONT_FORGET_LEFTHOOK": "when making changes in scripts check out if .lefthook.yml should be updated",
    "pm:update": "bunx npm-check-updates -i && bun update",
    "check": "bun --bun tsc --p HallOfFame/UI --noEmit && biome check",
    "build": "webpack --config HallOfFame/UI/webpack.config.js --progress",
    "dev": "webpack --config HallOfFame/UI/webpack.config.js --progress --watch"
  },
  "dependencies": {
    "@csmodding/urbandevkit": "^0.1.0",
    "classnames": "^2.5.1",
    "common-tags": "^1.8.2",
    "date-fns": "^3.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "~1.8.3",
    "@types/common-tags": "^1.8.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "css-loader": "^7.1.2",
    "lefthook": "^1.7.2",
    "mini-css-extract-plugin": "^2.9.0",
    "sass": "^1.77.8",
    "sass-loader": "^14.2.1",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.3",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  },
  "trustedDependencies": ["@biomejs/biome", "lefthook"]
}
